{
  "name": "waspleditorlocker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "build": "vite build --mode production",
    "dev:frontend": "vite",
    "dev:server": "node server/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "dev:env:docker": "dotenv -e .env.docker -- concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "prod:docker": "dotenv -e .env.production -- concurrently \"npm run build\" \"npm run start\"",
    "update-owners": "node ./server/utils/updateOwner.js",
    "update-items-owners": "node ./server/utils/updateItemsOwner.js",
    "update-publications-owner": "node ./server/utils/updatePublicationsOwner.js",
    "update-groups-owner": "node ./server/utils/updateGroupsOwner.js",
    "update-results-owner": "node ./server/utils/updateOwnerResults.js",
    "generate:token": "node ./src/utils/generateSystemToken.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@joeattardi/emoji-button": "^4.6.4",
    "@jsdawn/vue3-tinymce": "^2.0.4",
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-3": "^0.5.1",
    "bootstrap-vue-next": "^0.26.7",
    "chart.js": "^4.4.9",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fast-xml-parser": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "konva": "^9.3.20",
    "luxon": "^3.5.0",
    "marked": "^15.0.8",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.2",
    "papaparse": "^5.5.1",
    "pinia": "^2.1.7",
    "quill": "^2.0.3",
    "tabulator-tables": "^6.3.0",
    "use-image": "^1.1.1",
    "uuid": "^11.0.3",
    "validator": "^13.12.0",
    "vue": "^3.4.38",
    "vue-chartjs": "^5.3.2",
    "vue-good-table-next": "^0.2.2",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "vite": "^5.4.2"
  }
}
